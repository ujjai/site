<script>
    const theitems=document.querySelectorAll('.item')

    theitems.forEach(theitem =>{
        theitem.addEventListener('mouseover', () => {
            console.log(theitem.childNodes[1].classList);
            theitem.childNodes[1].classList.add('img-dark');
        })

        theitem.addEventListener('mouseout', () => {
            console.log(theitem.childNodes[1].classList);
            theitem.childNodes[1].classList.remove('img-dark');
        })
    })

</script>



